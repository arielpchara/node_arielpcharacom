{
    "name": "arielpcharacom",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "start": "yarn run build && babel-node --presets es2015 index.js",
        "dev": "nodemon --exec \"babel-node --presets es2015,react index.js\" --watch index.js",
        "build": "yarn run lessc && yarn run bundle",
        "lessc": "lessc --clean-css src/index.less dist/bundle.css",
        "bundle": "browserify src/index.js -o dist/bundle.js -v",
        "watchify": "watchify src/index.js -o dist/bundle.js -v"
    },
    "dependencies": {
        "babel-cli": "^6.24.0",
        "babel-plugin-transform-object-rest-spread": "^6.23.0",
        "babel-preset-es2015": "^6.24.0",
        "babel-preset-latest": "^6.24.0",
        "babel-preset-react": "^6.23.0",
        "babelify": "^7.3.0",
        "brfs": "^1.4.3",
        "concurrently": "^3.4.0",
        "dotenv": "^4.0.0",
        "express": "^4.15.2",
        "isomorphic-fetch": "^2.2.1",
        "less": "^2.7.2",
        "less-plugin-clean-css": "^1.5.1",
        "react": "^15.4.2",
        "react-dom": "^15.4.2",
        "react-redux": "^5.0.3",
        "react-router-dom": "^4.0.0",
        "react-router-redux": "next",
        "redux": "^3.6.0",
        "redux-actions": "^2.0.1",
        "redux-logger": "^3.0.0",
        "redux-thunk": "^2.2.0",
        "watchify": "^3.9.0",
        "yarn": "^0.22.0"
    },
    "browserify": {
        "transform": [
            [
                "babelify",
                {
                    "presets": [
                        "es2015",
                        "react"
                    ],
                    "plugins": [
                        "transform-object-rest-spread"
                    ]
                }
            ]
        ]
    }
}